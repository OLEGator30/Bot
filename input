int $a=2*3*5*13*5, $b=7*3*5*17;

print("a = ",$a,"; ","b = ",$b,"; ");

@start:
if ($a<$b)
{
	int $temp;
	$temp=$a;
	$a=$b;
	$b=$temp;	
}
$a=$a-$b;
if ($a)
	goto @start;

print("GCD(a,b) = ",$b," |");

int $i=0,$arr[5];

@loop1:
$arr[$i]=$i;
$i=$i+1;
if ($i-5)
	goto @loop1;

$i=0;

@loop2:
print(" ",$arr[$i]);
$i=$i+1;
if ($i-5)
	goto @loop2;
